<div class="row">
    <table id="table">
        <tr>
            <th>Matrícula</th>
            <th>Usuário</th>
            <th>Administrativo</th>
            <th>Ativo</th>
            <th>Editar</th>
        </tr>
        <% appData.forEach(appData => { %>
            <tr>
                <td><%=appData.id%></td>
                <td class="userName"><%=appData.userName[0].toUpperCase() + appData.userName.substr(1)%></td>
                <% if (appData.super > 0) { %>
                    <td><a href="#"><i data-super="0" data-super-id="<%=appData.id%>" class="material-icons blue-text super">check</i></a></td>
                <% } else { %>
                    <td><a href="#"><i data-super="1" data-super-id="<%=appData.id%>" class="material-icons blue-text super">check_box_outline_blank</i></a></td>
                <% } %>
                <% if (appData.status > 0) { %>
                    <td><a href="#"><i data-status="0" data-status-id="<%=appData.id%>" class="material-icons blue-text status">check</i></a></td>
                <% } else { %>
                    <td><a href="#"><i data-status="1" data-status-id="<%=appData.id%>" class="material-icons blue-text status">check_box_outline_blank</i></a></td>
                <% } %>
                <td><a href="#"><i class="material-icons blue-text">edit</i></a></td>
            </tr>
        <% }); %>
    </table>
</div>